<table id ="usuarios">
	<thead>
		<tr>
			<th>Nombre</th>
			<th>RIF</th>
			<th>Direccion</th>
			<th>Correo</th>
			<th>Eliminar</th>
		</tr>
	</thead>
	<tbody>
		<% @usuarios.each do |usuario| %>
			<tr>
				<td><%= usuario.nombre %></td>
				<td><%= usuario.rif %></td>
				<td><%= usuario.direccion %></td>
				<td><%= usuario.correo %></td>
				<td><%=link_to "Eliminar" ,{:action=>"eliminar_usuario",:usuario_id=>usuario.id_usuario},:class=>"link_negro",:confirm=>"Esta usted seguro?"%></td>
			</tr>
		<% end %>
	</tbody>
</table>

